def es_primo(num):
    if num < 2:
        return False
    for i in range(2, int(math.sqrt(num)) + 1):
        if num % i == 0:
            return False
    return True

def primos_en_rango(inicio, fin):
    return [num for num in range(inicio, fin + 1) if es_primo(num)]

# Ejemplo de uso
inicio, fin = 10, 50
print(f"Los nÃºmeros primos entre {inicio} y {fin} son: {primos_en_rango(inicio, fin)}")
